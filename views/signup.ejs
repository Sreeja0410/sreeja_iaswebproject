<body  class="news-html">

  <form action="/signup" class=" w-100 m-auto" method="POST">

    <div class="box-news">
      <h4>Create Acount</h4>
      <%- include ("partials/messages") %>

      <div class="form-input-group">
        <input name="username" type="text" class="form-input" autocomplete="off" placeholder="Username" value="<%= typeof username != "undefined" ? username: "" %>" autofocus>
      </div>
      <div class="form-input-group">
        <input name="email" type="email" class="form-input" autocomplete="off" placeholder="Email" value="<%= typeof email != "undefined" ? email: "" %>">
      </div>
      <div class="form-input-group">
        <input name="password" type="password" class="form-input" placeholder="Password" value="<%= typeof password != "undefined" ? password: "" %>">
      </div>
      <div class="form-input-group">
        <input name="cpassword" type="password" class="form-input" placeholder="Confirm password" value="<%= typeof cpassword != "undefined" ? cpassword: "" %>">
      </div>
      <button class="btn btn-lg btn-secondary form-btn btn-signup" type="submit">Create account</button>
      <p class="form-text">Already have an account?<a class="form-link" id="SigninAcc" href="login"> Sign In</a>
      </p>
    </div>

    <p class="copyright">Â© Copyright 2023 Truly IAS</p>
  </form>

</body>

